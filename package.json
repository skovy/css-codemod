{
  "version": "0.0.0-dripip",
  "license": "MIT",
  "name": "css-codemod",
  "author": "Spencer Miskoviak",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "bin": {
    "css-codemod": "./dist/cli.js"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsup src/index.ts src/cli.ts --dts",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "clean": "rimraf dist",
    "css-codemod": "yarn build && ./dist/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "resolutions": {
    "**/tsdx/jest": "27.4.7",
    "**/tsdx/ts-jest": "27.1.3"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.13",
    "dripip": "^0.10.0",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bundle-require": "^3.0.2",
    "cac": "^6.7.12",
    "esbuild": "^0.14.14",
    "glob": "^7.2.0",
    "postcss": "^8.4.5"
  }
}
